<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="730.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.diegomonterroso.controller.MenuLoginController">
   <children>
      <ImageView fitHeight="750.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/Fondo%20Login.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfUser" layoutX="68.0" layoutY="182.0" prefHeight="34.0" prefWidth="192.0" promptText="Ingresa tu usuario" />
      <PasswordField fx:id="pfPassword" layoutX="68.0" layoutY="266.0" prefHeight="34.0" prefWidth="192.0" promptText="Ingresa tu contraseña" />
      <Button fx:id="btnIniciar" layoutX="194.0" layoutY="541.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="42.0" prefWidth="110.0" text="Iniciar Sesión" />
      <Button fx:id="btnRegistrar" disable="true" layoutX="194.0" layoutY="604.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="42.0" prefWidth="110.0" text="Registrar Usuario" textAlignment="CENTER" wrapText="true" />
   </children>
</AnchorPane>
