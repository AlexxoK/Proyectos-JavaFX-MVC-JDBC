<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.diegomonterroso.controller.MenuTicketSoporteController">
   <children>
      <ImageView fitHeight="750.0" fitWidth="1200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/Fondo%20MenuTicketSoporte.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfTicketId" editable="false" layoutX="60.0" layoutY="280.0" prefHeight="25.0" prefWidth="186.0" promptText="Ticket Id" />
      <TextArea fx:id="taDescripcion" layoutX="60.0" layoutY="398.0" prefHeight="118.0" prefWidth="288.0" promptText="Ingrese la descripcion de su problema." wrapText="true" />
      <ComboBox fx:id="cmbEstatus" layoutX="60.0" layoutY="567.0" prefHeight="26.0" prefWidth="186.0" />
      <AnchorPane layoutX="386.0" layoutY="343.0">
         <children>
            <TableView fx:id="tblTickets" onMouseClicked="#cargarForm" prefHeight="348.0" prefWidth="682.0">
              <columns>
                <TableColumn fx:id="colTicketId" prefWidth="75.0" text="Ticket Id" />
                <TableColumn fx:id="colDescripcion" prefWidth="209.5999755859375" text="Descripcion" />
                  <TableColumn fx:id="colEstatus" prefWidth="102.4000244140625" text="Estatus" />
                  <TableColumn fx:id="colCliente" prefWidth="209.5999755859375" text="Cliente" />
                  <TableColumn fx:id="colFacturaId" prefWidth="83.99997558593748" text="Factura Id" />
              </columns>
            </TableView>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="60.0" layoutY="612.0">
         <children>
            <Button fx:id="btnGuardar" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="43.0" prefWidth="97.0" text="Guardar" />
         </children>
      </AnchorPane>
      <Button fx:id="btnVaciar" alignment="CENTER" layoutX="197.0" layoutY="612.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="43.0" prefWidth="97.0" text="Vaciar Form" wrapText="true" />
      <ComboBox fx:id="cmbCliente" layoutX="386.0" layoutY="279.0" prefHeight="26.0" prefWidth="186.0" />
      <ComboBox fx:id="cmbFactura" layoutX="698.0" layoutY="279.0" prefHeight="26.0" prefWidth="186.0" />
      <Button fx:id="btnRegresar" layoutX="14.0" layoutY="710.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Regresar" />
   </children>
</AnchorPane>
