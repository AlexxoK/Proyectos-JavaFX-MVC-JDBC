<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.diegomonterroso.controller.MenuDetalleFacturaController">
   <children>
      <ImageView fitHeight="750.0" fitWidth="1200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/Fondo%20MenuDetalleFactura.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfDetalleFacturaId" editable="false" layoutX="89.0" layoutY="244.0" prefHeight="35.0" prefWidth="263.0" promptText="DetalleFactura Id" />
      <ComboBox fx:id="cmbFactura" layoutX="89.0" layoutY="340.0" prefHeight="35.0" prefWidth="206.0" />
      <ComboBox fx:id="cmbProducto" layoutX="89.0" layoutY="454.0" prefHeight="35.0" prefWidth="206.0" />
      <TableView fx:id="tblDetalleFacturas" layoutX="400.0" layoutY="244.0" onMouseClicked="#cargarForm" prefHeight="378.0" prefWidth="638.0">
        <columns>
          <TableColumn fx:id="colDetalleFacturaId" prefWidth="75.0" text="detalleFacturaId" />
          <TableColumn fx:id="colFactura" prefWidth="283.20001220703125" text="Factura" />
            <TableColumn fx:id="colProducto" prefWidth="279.19989013671875" text="Producto" />
        </columns>
      </TableView>
      <Button fx:id="btnRegresar" layoutX="14.0" layoutY="710.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Regresar" />
      <Button fx:id="btnEliminar" alignment="CENTER" layoutX="400.0" layoutY="640.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="17.0" prefWidth="62.0" text="Eliminar" wrapText="true" />
      <TextField fx:id="tfDetalleFacturaBuscarId" layoutX="719.0" layoutY="639.0" prefHeight="26.0" prefWidth="264.0" promptText="Ingrese el Id del detalle a buscar" />
      <Button fx:id="btnBuscar" layoutX="989.0" layoutY="639.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Buscar" />
      <Button fx:id="btnGuardar" layoutX="89.0" layoutY="530.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="43.0" prefWidth="97.0" text="Guardar" />
      <Button fx:id="btnVaciar" alignment="CENTER" layoutX="197.0" layoutY="530.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="43.0" prefWidth="97.0" text="Vaciar" wrapText="true" />
   </children>
</AnchorPane>
